<p id="notice"><%= notice %></p>

<h1>Listing Sliders</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Descriptiong</th>
      <th>Descriptiond</th>
      <th>Picture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sliders.each do |slider| %>
      <tr>
        <td><%= slider.name %></td>
        <td><%= slider.descriptiong %></td>
        <td><%= slider.descriptiond %></td>
        <td><%= image_tag slider.picture.url(:thumb) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
